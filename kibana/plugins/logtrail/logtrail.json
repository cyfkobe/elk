{
    "version" : 2,
    "index_patterns" : [{      
        "es": {
            "default_index": "docker-localhost*"
        },
        "tail_interval_in_seconds": 1,
        "es_index_time_offset_in_seconds": 0,
        "display_timezone": "local",
        "display_timestamp_format": "YYYY年MM月DD日 HH:mm:ss",
        "max_buckets": 500,
        "default_time_range_in_days" : 0,
        "max_hosts": 100,
        "max_events_to_keep_in_viewer": 5000,
        "default_search": "",
        "fields" : {
            "mapping" : {
                "timestamp" : "@timestamp",
                "hostname" : "container.name",
                "program": "source",
                "message": "message"
            },
            "message_format": "{{{message}}}",
            "keyword_suffix" : ""
        },
        "color_mapping" : {
	        "field": "log_level",
            "mapping": {
                "ERROR": "#FF0000",
                "WARN": "#FFEF96",
                "DEBUG": "#B5E7A0",
                "TRACE": "#CFE0E8",
                "INFO": "#339999"
            }
	    }
	}]
}
